{
  "name": "feelstatic",
  "version": "0.1.0",
  "description": "Edit content and fields for static sites in Next.js",
  "author": "Mikkel Damm <mikkeldamm@hotmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build2": "tsup && tailwindcss -i ./src/styles.css -o ./dist/feelstatic.css",
    "dev2": "tsup --watch & tailwindcss -i ./src/styles.css -o ./dist/feelstatic.css --watch",
    "dev3": "tailwindcss -i ./src/styles.css -o ./dist/feelstatic.css",
    "build3": "pnpm run build:styles && pnpm run build:esm && pnpm run build:cjs",
    "build3:styles": "tailwindcss ./src/styles.css -o ./dist/feelstatic.css",
    "build3:esm": "tsc",
    "build3:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build": "npx swc ./src/index.tsx",
    "clean": "rm -rf dist",
    "test": "jest --passWithNoTests",
    "check": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts*\""
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.7",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@octokit/rest": "19.0.5",
    "encoding": "0.1.13",
    "jotai": "1.12.1",
    "js-base64": "3.7.3",
    "octokit-commit-multiple-files": "4.2.0",
    "react-icons": "4.7.1",
    "swr": "2.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.26",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "eslint": "8.31.0",
    "eslint-config-custom": "workspace:*",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "next": "13.1.1",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "3.2.4",
    "tsconfig": "workspace:*",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "next": "^13",
    "react": "^18",
    "react-dom": "^18"
  }
}
